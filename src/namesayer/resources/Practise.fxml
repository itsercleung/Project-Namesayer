<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>
<?import org.controlsfx.control.ToggleSwitch?>

<AnchorPane fx:id="mainRoot" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="namesayer.PractiseController">
   <children>
      <HBox prefHeight="500.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox prefHeight="400.0" prefWidth="200.0" stylesheets="@stylesheet/toolbar.css">
               <children>
                  <Button fx:id="practiseButton" mnemonicParsing="false" onAction="#practisePressed" prefHeight="60.0" prefWidth="200.0" stylesheets="@stylesheet/toolbar.css" text="Practise">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="60.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="testMicrophoneButton" mnemonicParsing="false" onAction="#testMicrophonePressed" prefHeight="60.0" prefWidth="200.0" stylesheets="@stylesheet/toolbar.css" text="Test Microphone">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="recordNameButton" mnemonicParsing="false" onAction="#recordNamePressed" prefHeight="60.0" prefWidth="200.0" stylesheets="@stylesheet/toolbar.css" text="Record New Name">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="exitButton" mnemonicParsing="false" onAction="#exitPressed" prefHeight="60.0" prefWidth="200.0" stylesheets="@stylesheet/toolbar.css" text="Exit">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="270.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
            <VBox prefHeight="0.0" prefWidth="800.0" stylesheets="@stylesheet/general.css">
               <children>
                  <Label alignment="CENTER" opacity="0.33" prefHeight="41.0" prefWidth="800.0" text="Practise">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="10.0" top="20.0" />
                     </VBox.margin>
                  </Label>
                  <HBox prefHeight="80.0" prefWidth="200.0">
                     <children>
                        <TextField fx:id="searchTextField" prefHeight="45.0" prefWidth="300.0" promptText="Search for name (case-insensitive)" stylesheets="@stylesheet/general.css">
                           <HBox.margin>
                              <Insets left="140.0" right="4.0" top="6.0" />
                           </HBox.margin>
                        </TextField>
                        <Line endX="-50.0" endY="-50.0" rotate="90.0" scaleY="3.0" startX="-100.0" startY="-50.0">
                           <HBox.margin>
                              <Insets top="28.0" />
                           </HBox.margin>
                        </Line>
                        <Button mnemonicParsing="false" prefHeight="45.0" prefWidth="140.0" stylesheets="@stylesheet/general.css" text="Upload Names">
                           <HBox.margin>
                              <Insets left="3.0" top="5.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </HBox>
                  <Label opacity="0.78" text="PRACTICE PLAYLIST">
                     <VBox.margin>
                        <Insets left="325.0" top="-10.0" />
                     </VBox.margin>
                  </Label>
                  <HBox prefHeight="330.0" prefWidth="600.0" stylesheets="@stylesheet/general.css">
                     <children>
                        <ListView fx:id="selectedNamesView" prefHeight="250.0" prefWidth="300.0" stylesheets="@stylesheet/general.css">
                           <HBox.margin>
                              <Insets left="240.0" right="-440.0" top="35.0" />
                           </HBox.margin>
                        </ListView>
                        <VBox prefHeight="250.0" prefWidth="300.0">
                           <children>
                              <ListView fx:id="searchNamesView" prefHeight="250.0" prefWidth="300.0" stylesheets="@stylesheet/general.css">
                                 <VBox.margin>
                                    <Insets top="-10.0" />
                                 </VBox.margin>
                              </ListView>
                           </children>
                           <HBox.margin>
                              <Insets left="40.0" />
                           </HBox.margin>
                        </VBox>
                     </children>
                     <VBox.margin>
                        <Insets top="-30.0" />
                     </VBox.margin>
                  </HBox>
                  <VBox prefHeight="100.0" prefWidth="100.0" stylesheets="@stylesheet/general.css">
                     <children>
                        <ToggleSwitch fx:id="toggleRandomise" prefWidth="120.0" stylesheets="@stylesheet/general.css" text="Randomise">
                           <font>
                              <Font size="14.0" />
                           </font>
                           <VBox.margin>
                              <Insets left="243.0" top="10.0" />
                           </VBox.margin>
                        </ToggleSwitch>
                        <HBox prefHeight="100.0" prefWidth="200.0">
                           <VBox.margin>
                              <Insets left="40.0" />
                           </VBox.margin>
                           <children>
                              <Button fx:id="playNames" mnemonicParsing="false" onAction="#pressedPlayNames" prefHeight="50.0" prefWidth="140.0" stylesheets="@stylesheet/general.css" text="Practise">
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                                 <HBox.margin>
                                    <Insets left="203.0" right="5.0" top="10.0" />
                                 </HBox.margin>
                              </Button>
                              <Button fx:id="clearButton" mnemonicParsing="false" onAction="#clearButtonPressed" prefHeight="50.0" prefWidth="140.0" stylesheets="@stylesheet/general.css" text="Clear">
                                 <font>
                                    <Font size="14.0" />
                                 </font>
                                 <HBox.margin>
                                    <Insets left="10.0" top="10.0" />
                                 </HBox.margin>
                              </Button>
                           </children>
                        </HBox>
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </VBox>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
